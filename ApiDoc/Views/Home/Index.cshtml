@{
    ViewData["Title"] = "";
}

@section StyleSheet
{
    <link rel="stylesheet" href="~/css/home.css" />

}

<div class="absolute-left-58  w-240 bg-light border-right">
    <div class="input-group input-group-sm article-search">
        <input id="txtQFolderName" type="text" class="form-control" placeholder="Search..." aria-label="Search" aria-describedby="basic-addon2" autocomplete="off">
        <div class="input-group-append cur-pointer" onclick="btnSearchFolder_Click()">
            <span class="input-group-text" ><i class="zi zi_searchBlack"></i></span>
        </div>
    </div>
    <div id="treeview7" class="ml-1"></div>
</div>

<div class="absolute-right-58 w-width-240" style="padding:0;">
 
            <iframe id="myIframeList" src="~/Interface/Index" frameborder="0" scrolling="yes" style="width:100%;height: 100%;"></iframe>
        
</div>
<input type="hidden" />

<!-- 接口窗口 -->
<div class="modal" id="myModalInterface" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
    <div class="modal-dialog modal-xl " role="document" >
        <div id="myModalContent" class="modal-content" style="position:fixed; left:0px; top:0px; ">
            <div class="modal-title bg-light border-bottom text-left">
                <button class="btn" data-dismiss="modal"><span class="fa fa-reply" aria-hidden="true"></span></button>
            </div>
            <div class="modal-body p-0">
                <iframe id="myIframe" src="" frameborder="0" scrolling="yes" style="width:100%;height: 100%;"></iframe>
            </div>
            
        </div>
    </div>
</div>

@section Scripts
{
    <script src="~/js/bootstrap.bundle.js"></script>
    <script src="~/js/bootstrap-treeview.js"></script>
    <script src="~/js/bootstrap-closable-tab.js"></script>
    <script src="~/js/home.js"></script>

}


